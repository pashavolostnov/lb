import matplotlib.pyplot as plt
from numpy import *
import math

def fun(x):
  # cos(3*x-1)+x - function
  f = x**1
  return f

x = linspace(-5, 5, 1000)
y = cos(3*x-1)+x
yx = fun(x)


plt.plot(x, y, label='cos(3*x-1)+x’)
plt.plot(x, yx, label='x**1')
plt.legend()
plt.title('Розширення Тейлора')
plt.grid()
plt.show()
