import math
import numpy as np
from numpy import *
import matplotlib.pyplot as plt

def f(x,y):
    return x+sin(y/sqrt(1.3))
  
x0=1.8
p=2.8
h=0.1
x=np.arange(x0,p+h,h)
n=len(x)-1
y=np.empty(n+1)
y[0]=2.6

for i in range(n):
    y[i+1]=y[i]+f(x[i],y[i])*h

print('xi=',x)
print('yi=',y)

plt.plot(x, y,'bx-')
plt.xlabel('x') 
plt.ylabel('y')
plt.title('Euler method') 
plt.legend(['x+sin(y/sqrt(1.3)'])
plt.grid()
plt.show()
